{
  "name": "scrummer",
  "displayName": "scrummer",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:clickup.setToken",
    "onCommand:clickup.deleteToken",
    "onCOmmand:clickup.updateToken",
    "onCommand:clickup.addTask",
    "onCommand:clickup.deleteTask",
    "onCommand:clickup.editTask",
    "onCommand:Scrummer.hello",
    "onCommand:Scrummer.testing",
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "clickup.setToken",
        "category": "Click Up",
        "title": "Set Click Up Token",
        "icon": {
          "light": "media/token_setup_light.svg",
          "dark": "media/token_setup_dark.svg"
        }
      },
      {
        "command": "clickup.deleteToken",
        "category": "Click Up",
        "title": "Delete Click Up Token",
        "icon": {
          "light": "media/token_delete_light.svg",
          "dark": "media/token_delete_dark.svg"
        }
      },
      {
        "command": "clickup.updateToken",
        "category": "Click Up",
        "title": "Edit Click Up Token",
        "icon": {
          "light": "media/token_setup_light.svg",
          "dark": "media/token_setupt_dark.svg"
        }
      },
      {
        "command": "clickup.addTask",
        "category": "Click Up",
        "title": "Add Task",
        "icon": {
          "light": "media/task_add_light.svg",
          "dark": "media/task_add_dark.svg"
        }
      },
      {
        "command": "clickup.deleteTask",
        "category": "Click Up",
        "title": "Delete Task",
        "icon": {
          "light": "media/task_delete_light.svg",
          "dark": "media/task_delete_dark.svg"
        }
      },
      {
        "command": "clickup.editTask",
        "category": "Click Up",
        "title": "Edit Task",
        "icon": {
          "light": "media/task_edit_light.svg",
          "dark": "media/task_edit_dark.svg"
        }
      },
      {
        "command": "Scrummer.hello",
        "category": "Scrummer",
        "title": "Hello",
        "icon": {
          "light": "media/clickup_vscode_light.svg",
          "dark": "media/clickup_vscode_dark.svg"
        }
      },
      {
        "command": "Scrummer.testing",
        "category": "Scrummer Testing",
        "title": "Testing",
        "icon": "${beaker}"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "Scrummer-Views-Containers",
          "title": "Scrummer",
          "icon": "media/clickup_vscode_light.svg"
        }
      ]
    },
    "views": {
      "Scrummer-Views-Containers": [
        {
          "id": "Today-Tasks",
          "name": "Today Tasks",
          "icon": "media/tasks_today_light.svg"
        },
        {
          "id": "Complete-Tasks",
          "name": "Complete Tasks",
          "icon": "media/tasks_completed_light.svg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "clickup.js": "^3.0.2"
  }
}